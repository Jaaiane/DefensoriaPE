{
  "expo": {
    "name": "DefensoriaPE",
    "slug": "DefensoriaPE",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "DefensoriaPE",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/expo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/expo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jaiane.DefensoriaPE"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b809ecbe-b700-4e07-a0e9-db996ed5a12b"
      }
    }
  }
}
